    "version": "0.2.0",
    "configurations": [
        {
            "name": "Bisector",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\example\\foo_bisector.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "bisect",
                "d8a3a9c862cea0e2d7cc8267a31bd5f4721ff739",
                "6a7d094f8562601fe2f981667013273aefd47ae6"
            ]
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}